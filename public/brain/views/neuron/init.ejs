<h2>neurons</h2>
<table cellspacing='0px'>
	<thead>
	<tr>
		<% for(var attr in Brain.Models.Neuron.attributes){%>
			<% if(attr == 'id') continue;%>
			<th><%= attr%> </th>	 
		<%}%>
		<th>Options</th>
	</tr>
	</thead>
	<tbody>
		<%= $.View('//brain/views/neuron/list',{neurons: neurons})%>
	</tbody>
</table>
<h2>New neuron</h2>
<form>
	<% for(var attr in Brain.Models.Neuron.attributes){ %>
		<% if(attr == 'id') continue;%>
		<div>
			<label><%= attr %> </label><br />
			<input type="text" value="" name="<%= attr %>"/>
		</div>
	<%}%>
	<input type='submit' value='Create'/>
</form>